{
    "root": true,
    "extends": "eslint:recommended",
    "env": {
        "es6": true
    },
    "rules": {
        "eqeqeq": ["error", "always"],
        "quotes": ["error", "single"],
        "arrow-spacing": "error",
        "no-var": "error",
        "prefer-const": "error",
        "prefer-template": "error",
        "no-unused-vars": ["error", { "args": "none" }],
        "no-console": "off"
    }
}
